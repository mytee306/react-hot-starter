{
  "name": "react-hot-starter",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "prerender": "react-snapshot",
    "build:prerender": "run-s build prerender",
    "test:all": "react-app-rewired test",
    "test": "npm run test:all -- --testPathIgnorePatterns puppeteer",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.{ts,tsx}",
    "cpd": "jscpd src --ignore src/assets/img/*",
    "ci": "cross-env NODE_OPTIONS=--max_old_space_size=4096 CI=true run-p cpd lint test build --print-label --print-name --aggregate-output",
    "ci:concurrently": "cross-env NODE_OPTIONS=--max_old_space_size=4096 CI=true concurrently --kill-others-on-fail --names cpd,lint,test,build --prefix-colors white,cyan,yellow,magenta npm:cpd npm:lint npm:test npm:build",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o .out",
    "storybook:preview": "http-server .out",
    "storybook:deploy": "storybook-to-ghpages",
    "deploy:surge": "surge build --domain https://react-hot-starter-dev.surge.sh",
    "deploy": "firebase deploy --only hosting",
    "deploy:firebase": "firebase deploy",
    "serve:build": "serve -s build",
    "serve": "npm run serve:build",
    "http-serve:build": "http-server build",
    "commitlint": "commitlint --from HEAD~1",
    "cypress:run": "cypress run",
    "cypress:open": "cypress open"
  },
